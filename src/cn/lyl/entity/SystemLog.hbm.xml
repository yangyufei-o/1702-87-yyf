<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.tutorial.hbm">


	<class name="cn.lyl.entity.SystemLog">
		<id name="s_id">
			<generator class="native"></generator>
		</id>
		<property name="s_time" length="30"></property>
		<property name="s_operator" length="20"></property>
		<property name="s_info"></property>
		<property name="s_extend1"></property>
		<property name="s_extend2"></property>
		<property name="s_extend3"></property>
	</class>

</hibernate-mapping>
