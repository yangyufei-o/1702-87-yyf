<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.tutorial.hbm">

	<class name="cn.lyl.entity.LinkMan" table="LinkMan">
		<id name="l_code">
		
		</id>
		<many-to-one name="customer" class="cn.lyl.entity.Customer" column="custcode"></many-to-one>
		
		<property name="l_name" length="20"></property>
		<property name="l_nickname" length="20"></property>
		<property name="l_id" length="20"></property>
		<property name="l_sex" length="20"></property>
		<property name="l_birth" length="20"></property>
		<property name="l_birthday" length="20"></property>
		<property name="l_duty" length="20"></property>
		<property name="l_tel" length="20"></property>
		<property name="l_phone" length="20"></property>
		<property name="l_address" length="20"></property>
		<property name="l_mail" length="20"></property>
		<property name="l_qq" length="20"></property>
		<property name="l_like" length="20"></property>
		<property name="l_job" length="20"></property>
		<property name="l_extend1"></property>
		<property name="l_extend2"></property>
		<property name="l_extend3"></property>
		<property name="l_extend4"></property>
		<property name="l_extend5"></property>
	</class>

</hibernate-mapping>
